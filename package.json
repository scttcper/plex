{
  "name": "@ctrl/plex",
  "version": "3.9.2",
  "description": "plex api client in typescript using ofetch",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "scttcper/plex",
  "type": "module",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "keywords": [
    "plex",
    "typescript"
  ],
  "scripts": {
    "lint": "oxlint . && prettier --check . --experimental-cli",
    "lint:fix": "oxlint . --fix && prettier --write . --log-level=error --experimental-cli",
    "prepare": "npm run build",
    "build": "tsc",
    "build:docs": "typedoc",
    "test": "vitest run",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit --outputFile=./junit.xml",
    "bootstraptest": "tsx scripts/bootstraptest.ts",
    "teardowntest": "tsx scripts/teardowntest.ts",
    "add-media": "tsx scripts/add-media.ts",
    "test-cleanup": "tsx scripts/test-cleanup.ts"
  },
  "dependencies": {
    "@ctrl/mac-address": "^3.1.0",
    "ofetch": "^1.4.1",
    "p-any": "^4.0.0",
    "type-fest": "^5.0.1",
    "ws": "^8.18.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@ctrl/oxlint-config": "1.2.8",
    "@ctrl/video-filename-parser": "5.4.1",
    "@sindresorhus/tsconfig": "8.0.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "24.7.2",
    "@types/ws": "8.18.1",
    "@types/xml2js": "0.4.14",
    "@types/yargs": "17.0.33",
    "@vitest/coverage-v8": "3.2.4",
    "execa": "9.6.0",
    "globby": "15.0.0",
    "make-dir": "5.1.0",
    "ora": "9.0.0",
    "oxlint": "1.22.0",
    "p-retry": "7.1.0",
    "prettier": "3.6.2",
    "tsx": "4.20.6",
    "typedoc": "0.28.14",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "yargs": "18.0.0"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "semi": true,
    "printWidth": 100,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrder": [
      "^node:.*$",
      "<THIRD_PARTY_MODULES>",
      "^(@ctrl)(/.*|$)",
      "^\\.\\./(?!.*\\.css$)",
      "^\\./(?!.*\\.css$)(?=.*/)",
      "^\\./(?!.*\\.css$)(?!.*/)"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": false
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.18.2"
}
